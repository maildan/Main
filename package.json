{
  "name": "cls",
  "version": "0.1.0",
  "private": true,
  "main": "src/main/main.js",
  "scripts": {
    "dev": "concurrently -n \"next,electron\" -c \"blue,green\" \"next dev\" \"wait-on -l tcp:3000 && cross-env NODE_ENV=development electron --expose-gc --js-flags=\\\"--expose-gc\\\" .\"",
    "start": "concurrently \"next start\" \"electron --expose-gc --js-flags=\\\"--expose-gc\\\" .\"",
    "build": "next build",
    "lint": "eslint --cache --config eslint.config.mjs \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --cache --fix --config eslint.config.mjs \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:strict": "eslint --cache --max-warnings=0 --config eslint.config.mjs \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix-all": "node scripts/fix-eslint-errors.cjs",
    "lint:check": "eslint --cache --max-warnings=0 --config eslint.config.mjs \"**/*.{js,jsx,ts,tsx}\"",
    "lint:guide": "echo \"ESLint 가이드를 확인하려면 docs/eslint-fix-guide.md 파일을 참조하세요.\"",
    "fix:eslint": "eslint --fix src/**/*.{js,jsx,ts,tsx}",
    "fix:eslint-config": "node scripts/fix-eslint-config.cjs",
    "ignore:eslint": "node scripts/disable-eslint-errors.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "analyze": "cross-env ANALYZE=true next build",
    "postbuild": "next-sitemap",
    "init": "npm install --legacy-peer-deps && npm run fix:eslint-config && npm run lint:fix-all && node scripts/setup-eslint.cjs && node scripts/setup-cicd.cjs",
    "init:fix-all": "npm run fix:eslint-config && npm run lint:fix-all && node scripts/setup-eslint.cjs",
    "init:ci": "npm ci --legacy-peer-deps && npm run typecheck && npm run lint:check && npm run build",
    "optimize:eslint": "node scripts/optimize-eslint-performance.cjs",
    "electron:dev": "cross-env NODE_ENV=development electron --expose-gc --js-flags=\"--expose-gc\" .",
    "electron:start": "electron --expose-gc --js-flags=\"--expose-gc\" .",
    "electron:build": "electron-builder",
    "electron:package": "npm run build && npm run electron:build",
    "build:native": "cd native-modules && cargo build --release --verbose",
    "build:native:debug": "cd native-modules && cargo build --verbose && npm run copy-native-debug",
    "copy-native": "node scripts/copy-native.js",
    "copy-native-debug": "node scripts/copy-native.js --debug",
    "postinstall": "node scripts/install-native.cjs",
    "deploy:prod": "node scripts/deploy-production.cjs",
    "deploy:staging": "node scripts/deploy-staging.cjs",
    "prepare": "husky install",
    "server": "next dev",
    "electron": "cross-env NODE_ENV=development electron --expose-gc --js-flags=\"--expose-gc\" .",
    "dev:next": "next dev",
    "dev:electron": "cross-env NODE_ENV=development electron --expose-gc --js-flags=\\\"--expose-gc\\\" .",
    "dev:debug": "concurrently -n \"next,electron\" -c \"blue,green\" \"next dev\" \"wait-on -l tcp:3000 && cross-env NODE_ENV=development ELECTRON_DEBUG=true electron --inspect=9229 --expose-gc --js-flags=\\\"--expose-gc\\\" .\""
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.0.1",
    "@mui/material": "^7.0.1",
    "@prisma/client": "^6.5.0",
    "@radix-ui/react-label": "^2.1.4",
    "@radix-ui/react-separator": "^1.1.4",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-switch": "^1.2.2",
    "@shadcn/ui": "^0.0.4",
    "@types/recharts": "^1.8.29",
    "active-win": "^8.1.0",
    "better-sqlite3": "^11.9.1",
    "chart.js": "^4.4.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.7",
    "electron-is-dev": "^3.0.1",
    "fs": "^0.0.1-security",
    "lucide-react": "^0.503.0",
    "mysql2": "^3.13.0",
    "napi-rs": "^0.2.6",
    "next": "^15.2.4",
    "node-gyp": "^9.4.1",
    "prisma": "^6.5.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.15.1",
    "tailwind-merge": "^3.2.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.8.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^8.57.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/better-sqlite3": "^7.6.12",
    "@types/lodash": "^4.17.16",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "autoprefixer": "^10.4.21",
    "cargo-cp-artifact": "^0.1.8",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "cypress": "^13.6.4",
    "electron": "^35.0.2",
    "eslint": "^9.23.0",
    "eslint-config-next": "^15.2.4",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.0.0",
    "globals": "^15.15.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.0",
    "next-sitemap": "^4.2.3",
    "null-loader": "^4.0.1",
    "postcss": "^8.5.3",
    "prettier": "^3.2.5",
    "tailwindcss": "^4.1.4",
    "typescript-eslint": "^8.29.0",
    "wait-on": "^8.0.3"
  },
  "description": "This is a [Next.js](https://nextjs.org) project bootstrapped with [`create-next-app`](https://nextjs.org/docs/app/api-reference/cli/create-next-app).",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "iohook": {
    "targets": [
      "node-108",
      "electron-120"
    ],
    "platforms": [
      "win32",
      "darwin"
    ],
    "arches": [
      "x64",
      "arm64"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maildan/Main.git"
  },
  "bugs": {
    "url": "https://github.com/maildan/Main/issues"
  },
  "homepage": "https://github.com/maildan/Main#readme",
  "directories": {
    "doc": "docs"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix --cache",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ]
  }
}
