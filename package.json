{
  "name": "typing-stats-app",
  "version": "0.1.0",
  "private": true,
  "main": "main.js",
  "scripts": {
    "dev": "concurrently \"npm run next:dev\" \"npm run electron:wait\"",
    "next:dev": "cross-env NEXT_TELEMETRY_DISABLED=1 NODE_OPTIONS='--max-old-space-size=4096' next dev",
    "electron:wait": "wait-on http://localhost:3000 && npm run electron:dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "electron:dev": "cross-env NODE_ENV=development electron --expose-gc --js-flags=\"--expose-gc\" .",
    "electron:start": "electron --expose-gc --js-flags=\"--expose-gc\" .",
    "electron:build": "electron-builder",
    "electron:package": "npm run build && npm run electron:build",
    "build:native": "cd native-modules && cargo build --release --verbose",
    "build:native:debug": "cd native-modules && cargo build --verbose && npm run copy-native-debug",
    "copy-native": "node scripts/copy-native.js",
    "copy-native-debug": "node scripts/copy-native.js --debug",
    "postinstall": "node scripts/install-native.js",
    "install:native": "node scripts/install-native.js",
    "dev:fast": "cross-env NEXT_TELEMETRY_DISABLED=1 NODE_OPTIONS='--max-old-space-size=8192 --no-warnings' NODE_ENV=development next dev --turbo",
    "dev:memory": "cross-env NODE_OPTIONS='--max-old-space-size=8192' npm run dev"
  },
  "dependencies": {
    "@prisma/client": "^6.5.0",
    "@types/recharts": "^1.8.29",
    "active-win": "^8.1.0",
    "better-sqlite3": "^11.9.1",
    "chart.js": "^4.4.8",
    "dotenv": "^16.4.7",
    "electron-is-dev": "^3.0.1",
    "mysql2": "^3.13.0",
    "napi-rs": "^0.3.10",
    "next": "15.2.2",
    "node-global-key-listener": "^0.1.1",
    "node-gyp": "^9.4.1",
    "prisma": "^6.5.0",
    "react": "^19.0.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.0.0",
    "recharts": "^2.15.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/better-sqlite3": "^7.6.12",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cargo-cp-artifact": "^0.1.8",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "electron": "^35.0.2",
    "wait-on": "^8.0.3"
  },
  "description": "This is a [Next.js](https://nextjs.org) project bootstrapped with [`create-next-app`](https://nextjs.org/docs/app/api-reference/cli/create-next-app).",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "iohook": {
    "targets": [
      "node-108",
      "electron-120"
    ],
    "platforms": [
      "win32",
      "darwin"
    ],
    "arches": [
      "x64",
      "arm64"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maildan/Main.git"
  },
  "bugs": {
    "url": "https://github.com/maildan/Main/issues"
  },
  "homepage": "https://github.com/maildan/Main#readme"
}
