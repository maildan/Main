{
  "name": "cls",
  "version": "0.1.0",
  "private": true,
  "main": "main.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"next dev\" \"cross-env NODE_ENV=development electron --expose-gc --js-flags=\\\"--expose-gc\\\" .\"",
    "start": "concurrently \"next start\" \"electron --expose-gc --js-flags=\\\"--expose-gc\\\" .\"",
    "build": "next build",
    "lint": "eslint --cache --max-warnings=0 --config eslint.config.mjs \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --cache --fix --config eslint.config.mjs \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:strict": "eslint --cache --max-warnings=0 --config eslint.config.mjs \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix-all": "node scripts/fix-eslint-errors.js",
    "lint:check": "eslint --cache --max-warnings=0 --config eslint.config.mjs \"**/*.{js,jsx,ts,tsx}\"",
    "lint:guide": "echo \"ESLint 가이드를 확인하려면 docs/eslint-fix-guide.md 파일을 참조하세요.\"",
    "fix:eslint": "node scripts/fix-eslint-errors.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "analyze": "cross-env ANALYZE=true next build",
    "postbuild": "next-sitemap",
    "init": "npm install --legacy-peer-deps && npm run lint:fix && node scripts/setup-eslint.js && node scripts/setup-cicd.js",
    "init:ci": "npm ci --legacy-peer-deps && npm run typecheck && npm run lint:check && npm run build",
    "electron:dev": "cross-env NODE_ENV=development electron --expose-gc --js-flags=\"--expose-gc\" .",
    "electron:start": "electron --expose-gc --js-flags=\"--expose-gc\" .",
    "electron:build": "electron-builder",
    "electron:package": "npm run build && npm run electron:build",
    "build:native": "cd native-modules && cargo build --release --verbose",
    "build:native:debug": "cd native-modules && cargo build --verbose && npm run copy-native-debug",
    "copy-native": "node scripts/copy-native.js",
    "copy-native-debug": "node scripts/copy-native.js --debug",
    "postinstall": "node scripts/install-native.js",
    "deploy:prod": "node scripts/deploy-production.js",
    "deploy:staging": "node scripts/deploy-staging.js",
    "prepare": "husky install"
  },
  "dependencies": {
    "@prisma/client": "^6.5.0",
    "@types/recharts": "^1.8.29",
    "active-win": "^8.1.0",
    "better-sqlite3": "^11.9.1",
    "chart.js": "^4.4.8",
    "dotenv": "^16.4.7",
    "electron-is-dev": "^3.0.1",
    "fs": "^0.0.1-security",
    "mysql2": "^3.13.0",
    "napi-rs": "^0.3.10",
    "next": "15.2.2",
    "node-global-key-listener": "^0.1.1",
    "node-gyp": "^9.4.1",
    "prisma": "^6.5.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.15.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@eslint/js": "^8.56.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/better-sqlite3": "^7.6.12",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "cargo-cp-artifact": "^0.1.8",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "cypress": "^13.6.4",
    "electron": "^35.0.2",
    "eslint": "^9.23.0",
    "eslint-config-next": "^15.2.4",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "globals": "^15.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.0",
    "next-sitemap": "^4.2.3",
    "prettier": "^3.2.5",
    "wait-on": "^8.0.3"
  },
  "description": "This is a [Next.js](https://nextjs.org) project bootstrapped with [`create-next-app`](https://nextjs.org/docs/app/api-reference/cli/create-next-app).",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "iohook": {
    "targets": [
      "node-108",
      "electron-120"
    ],
    "platforms": [
      "win32",
      "darwin"
    ],
    "arches": [
      "x64",
      "arm64"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maildan/Main.git"
  },
  "bugs": {
    "url": "https://github.com/maildan/Main/issues"
  },
  "homepage": "https://github.com/maildan/Main#readme",
  "directories": {
    "doc": "docs"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix --cache",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ]
  }
}
